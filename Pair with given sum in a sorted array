class Solution {
  public:
    int countPairs(vector<int> &arr, int target) {
        // Complete the function
        int cnt=0;
        for(int i=0;i<arr.size();i++){
            for(int j=i;j<arr.size();j++){
                if(arr[i]+arr[j]==target)
                   cnt++;
            }
        }
        return cnt;
    }
};
